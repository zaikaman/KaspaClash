# KaspaClash Environment Variables
# Copy this file to .env.local and fill in the values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Kaspa Network Configuration
NEXT_PUBLIC_KASPA_NETWORK=testnet
NEXT_PUBLIC_KASPA_RPC_URL=wss://testnet.kaspathon.com/wrpc

# Optional: Kaspa Explorer URL
NEXT_PUBLIC_KASPA_EXPLORER_URL=https://explorer.kaspa.org

# Optional: Relayer Service (for sponsored transactions)
RELAYER_PRIVATE_KEY=your-relayer-private-key

# Development
NODE_ENV=development
